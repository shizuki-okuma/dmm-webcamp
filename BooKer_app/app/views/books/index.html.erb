<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>
<h1>Books</h1>

<% @books.each do |book| %>

  <p>title</p>
  <span><%= book.title %></span>
  
<% end %>

<h1>New book</h1>
<%= form_with model: @book, url: '/books', method: :post do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Books' %>
<% end %>